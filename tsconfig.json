{
	"compileOnSave": false,
	"name": "storm-react-diagrams",
	"compilerOptions": {
		"suppressExcessPropertyErrors": true,
		"declaration": true,
		"module": "commonjs",
		"outDir": "./dist",
		"target": "es5",
		"strictNullChecks": false,
		"sourceMap": true,
		"skipLibCheck": true,
		"jsx": "react",
		"baseUrl": ".",
		"paths": {
			"storm-react-diagrams": ["src/main.ts"]
		},
		"lib": ["dom", "es2015"]
	},
	"include": ["src"],
	"exclude": ["node_modules", "**/*.spec.ts"]
}
